
@model Fan
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>




<form asp-action="SignUp" method="post">



    <div class="form-group">
        <label for"">Username</label>
        <input asp-for="SuperId" class="form-control"  aria-describedby="emailHelp" placeholder="Enter username">
        <span asp-validation-for="SuperId" class="test-danger"></span>
        <small id="emailHelp" class="form-text text-muted">We'll never share your information with anyone else.TRUST US:)</small>
    </div>


    <div class="form-group">
        <label for="exampleInputName">Name</label>
        <input asp-for="Name" class="form-control" placeholder="Name">
    </div>


    

    <div class="form-group">
        <label for="exampleInput">National id number</label>
        <input asp-for="NationalId" class="form-control" placeholder="National id">
    </div>

    <div class="form-group">
        <label asp-for="PhoneNum">Phone number</label>
        <input asp-for="PhoneNum" class="form-control" placeholder="Phone number">
    </div>

    <div class="form-group">
        <label asp-for="Birthdate">Birthdate</label>
        <input asp-for="Birthdate" class="form-control" placeholder="Month/Day/Year">
    </div>


    <div class="form-group">
        <label asp-for="Address">Address</label>
        <input asp-for="Address" class="form-control" placeholder="Address">
    </div>


    <br>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>




@section Scripts{

    @{
        <partial name="_ValidationScriptsPartial" />
    }
    }

    