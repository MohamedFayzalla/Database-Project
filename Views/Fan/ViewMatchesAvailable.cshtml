
@{
    ViewData["Title"] = "ViewMatchesAvailable";
}

<h1>ViewMatchesAvailable</h1>


@if (TempData["failed"] != null)
{
    <h2>@TempData["failed"]</h2>
}


@using (Html.BeginForm("ViewMatchesAvailable", "Fan"))
{





    <div class="form-group">
        <label>DATE For Match</label>
        <input type="datetime-local" name="time" id="time" class="form-control" placeholder="DATE">
    </div>



    <br>

    <button type="submit" class="btn btn-primary">Submit</button>

}



@section Scripts{

    @{
        <partial name="_ValidationScriptsPartial" />
    }
    }
