
@{
    ViewData["Title"] = "SystemAdmin";
   
}

<h1>SystemAdmin</h1>


@if (TempData["failed"] != null)
{
    <h2>@TempData["failed"]</h2>
}


@using (Html.BeginForm("SystemAdmin", "SignUp"))
{
    
    <div class="form-group">
        <label>Username</label>
        <input class="form-control" name="username" placeholder="Enter username">
        
        <small id="emailHelp" class="form-text text-muted">We'll never share your information with anyone else.TRUST US:)</small>
    </div>




    <div class="form-group">
        <label>Password</label>
        <input type="password" class="form-control" name="password" placeholder="password">
    </div>



    <br>
    <button type="submit" class="btn btn-primary">Register</button>






        
}

@section Scripts{

    @{
        <partial name="_ValidationScriptsPartial" />
    }
    }